<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cardsample"
    android:versionCode="2"
    android:versionName="1.0.1" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/fruit"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.cardsample.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="SingleColumnActivity"></activity>
        <activity android:name="MulticolumnActivity"></activity>
        <activity android:name="MultiSizedColumnActivity"></activity>
        <activity android:name="ViewPagerActivity"></activity>
        <activity android:name="ColorCardsActivity"></activity>
        <activity android:name=".orderedtreemap.PivotBiggestOrderedTreeMapActivity"></activity>
        <activity android:name=".orderedtreemap.PivotSizeOrderedTreeMapActivity"></activity>
        <activity android:name=".orderedtreemap.PivotMiddleOrderedTreeMapActivity"></activity>
    </application>

</manifest>
